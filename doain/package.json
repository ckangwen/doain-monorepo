{
  "name": "doain",
  "version": "0.1.0",
  "description": "doain客户端,主要提供项目入口,工具函数以及内部组件",
  "keywords": [
    "doain",
    "admin-management",
    "element-plus",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckangwen/doain",
    "directory": "packages/client"
  },
  "license": "MIT",
  "author": "ckangwen",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.js"
    },
    "./*": "./*",
    "./components": {
      "import": "./dist/components.js",
      "types": "./dist/components.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    },
    "./composables": {
      "import": "./dist/composables.js",
      "types": "./dist/composables.d.ts"
    },
    "./core": {
      "import": "./dist/core.js",
      "types": "./dist/core.d.ts"
    },
    "./toolkit": {
      "import": "./dist/toolkit.js",
      "types": "./dist/toolkit.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "types": "./dist/node.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./globals": {
      "types": "./globals.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "vite build",
    "test": "vitest"
  },
  "dependencies": {
    "@charrue/layout-next": "^1.0.5",
    "@charrue/node-toolkit": "^0.4.0",
    "@charrue/schema-form-next": "^1.2.1",
    "@charrue/schema-table-next": "^1.0.3",
    "@charrue/vite-plugin-pages": "^0.28.0",
    "@charrue/vite-plugin-pages-extend": "^0.1.0",
    "@charrue/vite-plugin-vue-layouts": "^0.8.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@vitejs/plugin-vue": "4.1.0",
    "@vitejs/plugin-vue-jsx": "2.1.1",
    "@vueuse/core": "^9.13.0",
    "axios": "^0.26.1",
    "compressorjs": "^1.2.1",
    "element-plus": "^2.3.1",
    "element-resize-detector": "^1.2.4",
    "js-cookie": "^3.0.1",
    "mitt": "^3.0.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "rollup-plugin-external-globals": "^0.7.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "4.2.1",
    "vite-plugin-inspect": "^0.7.18",
    "vue": "3.2.47",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@charrue/toolkit": "^0.14.1",
    "@charrue/types": "^0.1.0",
    "@charrue/vitest-service": "^0.0.2",
    "@types/element-resize-detector": "^1.1.3",
    "@types/js-cookie": "^3.0.3",
    "@types/qs": "^6.9.7",
    "@unocss/reset": "^0.47.6",
    "jsdom": "^20.0.3",
    "tsup": "^6.7.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}